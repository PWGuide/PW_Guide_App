<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/background"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        tools:context=".InsideNavigation">


        <AutoCompleteTextView
            android:id="@+id/build_list1"
            android:layout_width="230dp"
            android:layout_height="65dp"
            android:layout_marginStart="128dp"
            android:layout_marginTop="12dp"
            android:completionHint="Wybierz budynek"
            android:hint="@string/select_build"
            android:minHeight="48dp"
            android:textColor="@color/button_text"
            android:textColorHint="@color/button_text"
            android:visibility="visible"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView5" />

        <ImageView
            android:id="@+id/drop_down4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignTop="@+id/build_list1"
            android:layout_alignRight="@+id/build_list1"
            android:layout_alignBottom="@+id/build_list1"
            android:layout_marginStart="4dp"
            android:padding="5dp"
            android:src="@mipmap/outline_arrow_drop_down_black_24"
            app:layout_constraintBottom_toBottomOf="@+id/build_list1"
            app:layout_constraintStart_toEndOf="@+id/build_list1"
            app:layout_constraintTop_toTopOf="@+id/build_list1" />

        <AutoCompleteTextView
            android:id="@+id/build_list2"
            android:layout_width="230dp"
            android:layout_height="65dp"
            android:layout_marginStart="128dp"
            android:layout_marginTop="12dp"
            android:completionHint="Wybierz budynek"
            android:hint="@string/select_build"
            android:minHeight="48dp"
            android:textColor="@color/button_text"
            android:textColorHint="@color/button_text"
            android:visibility="visible"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView4" />

        <ImageView
            android:id="@+id/drop_down3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignTop="@+id/build_list2"
            android:layout_alignRight="@+id/build_list2"
            android:layout_alignBottom="@+id/build_list2"
            android:layout_marginStart="2dp"
            android:padding="5dp"
            android:src="@mipmap/outline_arrow_drop_down_black_24"
            app:layout_constraintBottom_toBottomOf="@+id/build_list2"
            app:layout_constraintStart_toEndOf="@+id/build_list2"
            app:layout_constraintTop_toTopOf="@+id/build_list2" />

        <TextView
            android:id="@+id/textView4"
            android:layout_width="342dp"
            android:layout_height="55dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="32dp"
            android:fontFamily="@font/alfa_slab_one"
            android:text="@string/Wybierz_punkt_początkowy"
            android:textColor="@color/button"
            android:textSize="20sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <Button
            android:id="@+id/b_rozpocznij"
            android:layout_width="175dp"
            android:layout_height="55dp"
            android:layout_marginTop="36dp"
            android:backgroundTint="@color/button"
            android:text="Rozpocznij"
            android:textColor="@color/button_text"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/hall_list1" />

        <TextView
            android:id="@+id/textView5"
            android:layout_width="342dp"
            android:layout_height="55dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="52dp"
            android:fontFamily="@font/alfa_slab_one"
            android:text="Wybierz salę końcową"
            android:textColor="@color/button"
            android:textSize="20sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/hall_list2" />

        <TextView
            android:id="@+id/textView6"
            android:layout_width="72dp"
            android:layout_height="30dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="32dp"
            android:layout_marginEnd="10dp"
            android:fontFamily="@font/alfa_slab_one"
            android:text="Budynek:"
            android:textColor="@color/button"
            app:layout_constraintEnd_toStartOf="@+id/build_list2"
            app:layout_constraintHorizontal_bias="0.533"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView4" />

        <TextView
            android:id="@+id/textView7"
            android:layout_width="72dp"
            android:layout_height="30dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="36dp"
            android:layout_marginEnd="10dp"
            android:fontFamily="@font/alfa_slab_one"
            android:text="Budynek:"
            android:textColor="@color/button"
            app:layout_constraintEnd_toStartOf="@+id/build_list1"
            app:layout_constraintHorizontal_bias="0.533"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView5" />

        <TextView
            android:id="@+id/textView8"
            android:layout_width="72dp"
            android:layout_height="30dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="36dp"
            android:layout_marginEnd="10dp"
            android:fontFamily="@font/alfa_slab_one"
            android:text="Sala:"
            android:textColor="@color/button"
            app:layout_constraintEnd_toStartOf="@+id/hall_list2"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView6" />

        <TextView
            android:id="@+id/textView9"
            android:layout_width="72dp"
            android:layout_height="30dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="36dp"
            android:layout_marginEnd="10dp"
            android:fontFamily="@font/alfa_slab_one"
            android:text="Sala:"
            android:textColor="@color/button"
            app:layout_constraintEnd_toStartOf="@+id/hall_list1"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView7" />

        <AutoCompleteTextView
            android:id="@+id/hall_list1"
            android:layout_width="230dp"
            android:layout_height="65dp"
            android:layout_marginStart="130dp"
            android:completionHint="Wybierz salę"
            android:hint="@string/select_hall"
            android:minHeight="48dp"
            android:textColor="@color/button_text"
            android:textColorHint="@color/button_text"
            android:visibility="visible"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/build_list1" />

        <ImageView
            android:id="@+id/drop_down6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignTop="@+id/hall_list1"
            android:layout_alignRight="@+id/hall_list1"
            android:layout_alignBottom="@+id/hall_list1"
            android:layout_marginStart="2dp"
            android:padding="5dp"
            android:src="@mipmap/outline_arrow_drop_down_black_24"
            app:layout_constraintBottom_toBottomOf="@+id/hall_list1"
            app:layout_constraintStart_toEndOf="@+id/hall_list1"
            app:layout_constraintTop_toTopOf="@+id/hall_list1" />

        <AutoCompleteTextView
            android:id="@+id/hall_list2"
            android:layout_width="230dp"
            android:layout_height="65dp"
            android:layout_marginStart="130dp"
            android:completionHint="Wybierz salę"
            android:hint="@string/select_hall"
            android:minHeight="48dp"
            android:textColor="@color/button_text"
            android:textColorHint="@color/button_text"
            android:visibility="visible"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/build_list2" />

        <ImageView
            android:id="@+id/drop_down5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignTop="@+id/hall_list2"
            android:layout_alignRight="@+id/hall_list2"
            android:layout_alignBottom="@+id/hall_list2"
            android:layout_marginStart="2dp"
            android:padding="5dp"
            android:src="@mipmap/outline_arrow_drop_down_black_24"
            app:layout_constraintBottom_toBottomOf="@+id/hall_list2"
            app:layout_constraintStart_toEndOf="@+id/hall_list2"
            app:layout_constraintTop_toBottomOf="@+id/build_list2" />
    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>