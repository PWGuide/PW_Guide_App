<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <androidx.constraintlayout.widget.ConstraintLayout
        tools:context=".MenuActivity"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/background">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginTop="16dp"
            android:background="@color/background"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:context=".MenuActivity">


            <TextView
                android:id="@+id/textView10"
                android:layout_width="376dp"
                android:layout_height="51dp"
                android:layout_marginStart="12dp"
                android:layout_marginTop="88dp"
                android:fontFamily="@font/alfa_slab_one"
                android:text="Wybierz końcową salę"
                android:textColor="@color/button"
                android:textSize="31dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <AutoCompleteTextView
                android:id="@+id/build_list3"
                android:layout_width="230dp"
                android:layout_height="65dp"
                android:layout_marginStart="128dp"
                android:completionHint="Wybierz budynek"
                android:hint="@string/select_build"
                android:minHeight="48dp"
                android:textColor="@color/button_text"
                android:textColorHint="@color/button_text"
                android:visibility="visible"
                app:layout_constraintBaseline_toBaselineOf="@+id/textView11"
                app:layout_constraintStart_toStartOf="parent" />

            <ImageView
                android:id="@+id/drop_down"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignTop="@+id/build_list3"
                android:layout_alignRight="@+id/build_list3"
                android:layout_alignBottom="@+id/build_list3"
                android:layout_marginStart="4dp"
                android:padding="5dp"
                android:src="@mipmap/outline_arrow_drop_down_black_24"
                app:layout_constraintBottom_toBottomOf="@+id/build_list3"
                app:layout_constraintStart_toEndOf="@+id/build_list3"
                app:layout_constraintTop_toTopOf="@+id/build_list3" />

            <TextView
                android:id="@+id/textView11"
                android:layout_width="81dp"
                android:layout_height="30dp"
                android:layout_marginStart="30dp"
                android:layout_marginTop="80dp"
                android:layout_marginEnd="10dp"
                android:fontFamily="@font/alfa_slab_one"
                android:text="Budynek:"
                android:textColor="@color/button"
                android:textSize="16sp"
                app:layout_constraintEnd_toStartOf="@+id/build_list3"
                app:layout_constraintHorizontal_bias="0.562"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView10" />

            <TextView
                android:id="@+id/textView12"
                android:layout_width="72dp"
                android:layout_height="30dp"
                android:layout_marginStart="30dp"
                android:layout_marginEnd="10dp"
                android:layout_marginBottom="319dp"
                android:fontFamily="@font/alfa_slab_one"
                android:text="Sala:"
                android:textColor="@color/button"
                android:textSize="16sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toStartOf="@+id/hall_list3"
                app:layout_constraintHorizontal_bias="0.666"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView11" />

            <AutoCompleteTextView
                android:id="@+id/hall_list3"
                android:layout_width="230dp"
                android:layout_height="65dp"
                android:layout_marginStart="130dp"
                android:completionHint="Wybierz salę"
                android:hint="@string/select_hall"
                android:minHeight="48dp"
                android:textColor="@color/button_text"
                android:textColorHint="@color/button_text"
                android:visibility="visible"
                app:layout_constraintBaseline_toBaselineOf="@+id/textView12"
                app:layout_constraintStart_toStartOf="parent" />

            <ImageView
                android:id="@+id/drop_down2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignTop="@+id/hall_list3"
                android:layout_alignRight="@+id/hall_list3"
                android:layout_alignBottom="@+id/hall_list3"
                android:layout_marginStart="2dp"
                android:padding="5dp"
                android:src="@mipmap/outline_arrow_drop_down_black_24"
                app:layout_constraintBottom_toBottomOf="@+id/hall_list3"
                app:layout_constraintStart_toEndOf="@+id/hall_list3"
                app:layout_constraintTop_toTopOf="@+id/hall_list3" />

            <Button
                android:id="@+id/b_rozpocznij2"
                android:layout_width="175dp"
                android:layout_height="55dp"
                android:layout_marginTop="40dp"
                android:backgroundTint="@color/button"
                android:text="Rozpocznij"
                android:textColor="@color/button_text"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.483"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/hall_list3" />

        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>